{
  "name": "floooo",
  "version": "1.0.0",
  "description": "Florr.io-like game",
  "scripts": {
    "build:client:public": "cd Sources/Client/public && esbuild Sources/Application.ts --bundle --minify --legal-comments=none --allow-overwrite --outfile=../../server/static/client.js",
    "build:client:private": "cd Sources/Client/private && zig build",
    "build:client:private:prod": "cd Sources/Client/private && zig build -Dproduction=true",
    "build:client:public:legacy": "tsc --noEmit & node esbuild.mjs",
    "build:server": "cd Sources/server && go build -ldflags=\"-s -w\" -trimpath cmd/flooooio/main.go",
    "start:server": "cd Sources/server && go run -ldflags=\"-s -w\" -trimpath cmd/flooooio/main.go",
    "start:server:debug": "cd Sources/server && go run cmd/flooooio/main.go"
  },
  "devDependencies": {
    "@basementuniverse/utils": "^1.7.0",
    "@basementuniverse/vec": "^2.2.2",
    "@stylistic/eslint-plugin": "^4.1.0",
    "@types/jsdom": "^21.1.7",
    "canvg": "^4.0.3",
    "dotenv": "^16.4.7",
    "esbuild": "0.24.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^15.13.0",
    "inquirer": "^12.2.0",
    "js-confuser": "^2.0.0",
    "lru_map": "^0.4.1",
    "mnemonist": "^0.40.3",
    "stackblur-canvas": "^2.7.0",
    "strict-event-emitter": "^0.5.1",
    "svgo": "^3.3.2",
    "typescript-eslint": "^8.18.0",
    "ws": "^8.18.0"
  },
  "dependencies": {
    "jsdom": "^26.1.0",
    "undici": "^7.10.0"
  }
}
